<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xin.online_exam_sys.dao.student.SLoginMapper">
    <select id="selectByIdAndPasswd" resultType="com.xin.online_exam_sys.pojo.entity.Student">
        SELECT * FROM student
        WHERE stu_id = #{userId} AND stu_passwd = #{userPasswd}
    </select>
</mapper>