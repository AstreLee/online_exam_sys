<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xin.online_exam_sys.dao.teacher.LoginMapper">
    <select id="selectByIdAndPasswd" resultType="teacher">
        SELECT * FROM teacher
        WHERE t_id = #{userId} AND t_passwd = #{userPasswd}
    </select>
</mapper>